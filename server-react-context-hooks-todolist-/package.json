{
  "name": "server-react-context-hooks-todolist",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "dev": "concurrently \"npm run server \" \"npm run client\"",
    "client": "npm start --prefix ../client-react-context-hooks-todolist",
    "client-install": "npm install --prefix client-react-context-hooks-todolist",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client-react-context-hooks-todolist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://https://github.com/rodmartinezmedina/Todo-App---React-Context-Hooks"
  },
  "author": "Rodrigo Martinez Medina",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rodmartinezmedina/Todo-App---React-Context-Hooks/issues"
  },

  "dependencies": {
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "json-parser": "^3.1.2",
    "mongoose": "^5.9.11",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.3"
  }
}
